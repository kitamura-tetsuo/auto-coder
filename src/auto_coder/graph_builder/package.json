{
  "name": "graph-builder",
  "version": "1.0.0",
  "description": "TypeScript and Python code analyzer for Neo4j graph database",
  "main": "dist/cli.js",
  "bin": {
    "graph-builder": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc && esbuild dist/cli.js --bundle --platform=node --outfile=dist/cli.bundle.js --external:fsevents",
    "test": "jest",
    "dev": "ts-node src/cli.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [
    "neo4j",
    "typescript",
    "python",
    "code-analysis",
    "graph-database"
  ],
  "author": "Auto-Coder Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "csv-writer": "^1.6.0",
    "glob": "^10.3.10",
    "ts-morph": "^21.0.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}