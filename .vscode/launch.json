    "version": "0.2.0",
    "configurations": [
        {
            "name": "Process Issues (Dry Run)",
            "type": "debugpy",
            "request": "launch",
            "module": "auto_coder.cli",
            "console": "integratedTerminal",
            "cwd": "/home/ubuntu/src2/outliner",
            "args": [
                "process-issues",
                "--dry-run"
            ],
            "python": "${command:python.interpreterPath}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Create Feature Issues",
            "type": "debugpy",
            "request": "launch",
            "module": "auto_coder.cli",
            "console": "integratedTerminal",
            "cwd": "/home/ubuntu/src2/outliner",
            "args": [
                "create-feature-issues"
            ],
            "python": "${command:python.interpreterPath}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Get Actions Logs",
            "type": "debugpy",
            "request": "launch",
            "module": "auto_coder.cli",
            "console": "integratedTerminal",
            "cwd": "/home/ubuntu/src2/outliner",
            "args": [
                "get-actions-logs",
                "--url",
                "https://github.com/kitamura-tetsuo/outliner/actions/runs/17006383413/job/48216559181?pr=502"
            ],
            "python": "${command:python.interpreterPath}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Auth Status",
            "type": "debugpy",
            "request": "launch",
            "module": "auto_coder.cli",
            "console": "integratedTerminal",
            "cwd": "/home/ubuntu/src2/outliner",
            "args": [
                "auth-status"
            ],
            "python": "${command:python.interpreterPath}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Process Issues (Live)",
            "type": "debugpy",
            "request": "launch",
            "module": "auto_coder.cli",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "process-issues"
            ],
            "python": "${command:python.interpreterPath}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Process Issues (Live) - Outliner",
            "type": "debugpy",
            "request": "launch",
            "module": "auto_coder.cli",
            "console": "integratedTerminal",
            "cwd": "/workspace",
            "args": [
                "process-issues",
                "--ignore-dependabot-prs",
                "--no-jules-mode",
                // "--backend",
                // "qwen",
                // "--backend",
                // "gemini",
                // "--backend",
                // "codex-mcp",
                // "--backend",
                // "codex",
                "--backend",
                "auggie",
                "--model-auggie",
                "sonnet4.5"
            ],
            "python": "${command:python.interpreterPath}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Fix to pass tests - Outliner",
            "type": "debugpy",
            "request": "launch",
            "module": "auto_coder.cli",
            "console": "integratedTerminal",
            "cwd": "/workspace",
            "args": [
                "fix-to-pass-tests",
                "--backend",
                "qwen",
                "--backend",
                "gemini",
                // // "--backend", "codex-mcp",
                // "--backend",
                // "codex",
                "--backend",
                "auggie",
                "--model-auggie",
                "sonnet4.5",
                "--max-attempts",
                "100000"
            ],
            "python": "${command:python.interpreterPath}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Check GraphRAG Services (All)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/check_graphrag_services.py",
            "console": "integratedTerminal",
            "cwd": "/workspace",
            "python": "${command:python.interpreterPath}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Check GraphRAG Services (Direct only)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/check_graphrag_services.py",
            "console": "integratedTerminal",
            "cwd": "/workspace",
            "args": [
                "--direct-only"
            ],
            "python": "${command:python.interpreterPath}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Check GraphRAG Services (MCP only)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/check_graphrag_services.py",
            "console": "integratedTerminal",
            "cwd": "/workspace",
            "args": [
                "--mcp-only"
            ],
            "python": "${command:python.interpreterPath}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        }
    ]
}